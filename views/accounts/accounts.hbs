<h1>User Accounts</h1>
<div class="accounts-container">
    {{#foreach accounts}}
    <div class="account">
        <p id="accountID">
            <a href="/accounts/{{account_ID}}">
                {{account_ID}}
            </a>
        </p>
        <p id="creditor">{{creditor}}</p><br>
        <p id="type">{{type}}</p>
        <p id="balance">{{balance}}</p><br>
    </div>
    {{/foreach}}
</div>