<h1>User Accounts</h1>
<div class="accounts-container">
    {{#each accounts}}
    <div class="account">
        <p id="accountID">
            <a href="/accounts/{{_id}}">{{_id}}</a>
        </p>
        <p id="creditor">{{creditor}}</p><br>
        <p id="type">{{type}}</p>
        <p id="balance">{{balance}}</p><br>
    </div>
    {{/each}}
</div>