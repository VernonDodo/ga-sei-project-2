<main>
    <h1>User Accounts</h1>
    <div class="accounts-container">
        {{#each accounts}}
        <div class="account">
            <div id="accountID">
                <a href="/accounts/{{account_ID}}">
                    {{account_ID}}
                </a>
            </div>
            <div id="creditor">{{creditor}}</div>
            <div id="type">{{type}}</div>
            <div id="balance">{{balance}}</div>
        </div>
        {{/each}}
    </div>
</main>